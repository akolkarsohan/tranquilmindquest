{
  "botName": "TranquilMindQuest-SupportBot",
  "description": "Mental health support chatbot for TranquilMindQuest",
  "locale": "en_US",
  "childDirected": false,
  "idleSessionTTLInSeconds": 300,
  "voiceId": "Joanna",
  "enableModelImprovements": true,
  "detectSentiment": true,
  "intents": [
    {
      "intentName": "GreetingIntent",
      "description": "Handles user greetings and initial contact",
      "sampleUtterances": [
        "hello",
        "hi",
        "hey",
        "good morning",
        "good afternoon",
        "good evening",
        "start",
        "begin",
        "help me",
        "I need help",
        "can you help me",
        "I'm here for support",
        "what can you do",
        "how are you",
        "good day"
      ],
      "intentConfirmationSetting": {
        "promptSpecification": {
          "messageGroups": [
            {
              "message": {
                "plainTextMessage": {
                  "value": "I understand you're greeting me. Is that correct?"
                }
              }
            }
          ],
          "maxRetries": 2,
          "allowInterrupt": true
        },
        "declinationResponse": {
          "messageGroups": [
            {
              "message": {
                "plainTextMessage": {
                  "value": "No problem. How else can I help you today?"
                }
              }
            }
          ]
        }
      },
      "fulfillmentCodeHook": {
        "enabled": true,
        "postFulfillmentStatusSpecification": {
          "successResponse": {
            "messageGroups": [
              {
                "message": {
                  "plainTextMessage": {
                    "value": "I'm here to help with your mental wellness journey."
                  }
                }
              }
            ]
          },
          "failureResponse": {
            "messageGroups": [
              {
                "message": {
                  "plainTextMessage": {
                    "value": "I'm having trouble processing that. Please try again."
                  }
                }
              }
            ]
          }
        }
      }
    },
