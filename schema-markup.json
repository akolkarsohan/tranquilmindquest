  "faqPage": {
    "@context": "https://schema.org",
    "@type": "FAQPage",
    "mainEntity": [
      {
        "@type": "Question",
        "name": "What is TranquilMindQuest?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "TranquilMindQuest is a comprehensive mental health and wellness platform that provides free resources, guided meditations, breathing exercises, and proven coping techniques to help people manage anxiety, stress, and depression."
        }
      },
      {
        "@type": "Question",
        "name": "Is TranquilMindQuest free to use?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "Yes, all our mental wellness resources, tools, and the AI chatbot are completely free to use. We believe mental health support should be accessible to everyone."
        }
      },
      {
        "@type": "Question",
        "name": "How quickly do you respond to messages?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "We typically respond to all messages within 24-48 hours. For urgent mental health concerns, please contact crisis resources immediately."
        }
      },
      {
        "@type": "Question",
        "name": "Is the AI chatbot a replacement for professional therapy?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "No, TranquilBot is designed to provide general wellness information and support. It's not a replacement for professional mental health care. For personalized treatment, please consult with a qualified mental health professional."
        }
      },
      {
        "@type": "Question",
        "name": "Is my conversation with the AI chatbot private?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "Yes, your conversations are private and confidential. We don't store personal information, and conversations are not shared with third parties. However, we may use anonymized data to improve our services."
        }
      },
      {
        "@type": "Question",
        "name": "What if I need immediate help?",
        "acceptedAnswer": {
          "@type": "Answer",
          "text": "If you're experiencing a mental health crisis, please contact emergency services immediately. You can call 988 (Suicide & Crisis Lifeline), text HOME to 741741 (Crisis Text Line), or contact your local emergency services."
        }
      }
    ]
  },

